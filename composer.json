{
    "name": "dizatech/pacman",
    "description": "A simple package manager for laravel apps.",
    "keywords": [
        "laravel",
        "module",
        "package",
        "package-manager",
        "commands",
        "laravel-module",
        "laravel-module-commands",
        "laravel8-package",
        "laravel8-package-manager",
        "PHP",
        "laravel8"
    ],
    "license": "MIT",
    "homepage": "https://github.com/dizatech/pacman",
    "authors": [
        {
            "name": "Sina Rajabpour",
            "email": "sina.rajabpour@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "^5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Dizatech\\Pacman\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dizatech\\Pacman\\PacmanServiceProvider"
            ]
        }
    }
}
